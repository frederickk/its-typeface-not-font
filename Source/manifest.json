{
	"manifest_version": 2,
	"name": "It's Typeface Not Font",
	"version": "1.0",
	"description": "Because it's \"typeface\" not \"font.\"",
	"homepage_url": "http://www.aiga.org/theyre-not-fonts/",
	"icons": {
		"16": "typeface-not-font-icon-16.png",
		"48": "typeface-not-font-icon-48.png",
		"128": "typeface-not-font-icon-128.png"
	},
	"content_scripts": [{
		"matches": [ "*://*/*" ],
		"js":      [ "content_script.js" ],
		"run_at":   "document_end"
	}]
}

